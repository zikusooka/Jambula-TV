    server {
        listen 8502;

        server_name  jambulatv_setup;
        root html/setup/;
        index  index.html index.htm index.php;
	autoindex on;
        access_log   /var/log/JambulaTV/.setup-nginx-DO-NOT-REMOVE.log;

        location / {
        }

        location ~ \.php$ {
            include  includes/php_params;
        }
 
    	location ~* \.(cgi|chi)$ {
            include  includes/cgi_params;
    	}  

    }

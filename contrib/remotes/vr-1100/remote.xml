      <start>PreviousMenu</start>
      <record>Record</record>
      <volumeplus>VolumeUp</volumeplus>
      <volumeminus>VolumeDown</volumeminus>
      <mute>Mute</mute>
      <power>ShutDown()</power>
      <myvideo>ActivateWindow(videos,files)</myvideo>
      <mymusic>ActivateWindow(music)</mymusic>
      <mypictures>ActivateWindow(pictures)</mypictures>
      <mytv>ActivateWindow(TVChannels)</mytv>
      <red>ActivateWindow(Home)</red>
      <green>ActivateWindow(MyVideos)</green>
      <yellow>ActivateWindow(MyMusic)</yellow>
      <blue>ActivateWindow(MyPictures)</blue>
      <zero>Number0</zero>
      <one>Number1</one>
      <two>JumpSMS2</two>
      <three>JumpSMS3</three>
      <four>JumpSMS4</four>
      <five>JumpSMS5</five>
      <six>JumpSMS6</six>
      <seven>JumpSMS7</seven>
      <eight>JumpSMS8</eight>
      <nine>JumpSMS9</nine>

      <!-- JambulaTV Buttons -->
      <language>ActivateWindow(TVRecordings)</language>
      <teletext>ActivateWindow(favourites)</teletext>
      <xbox>ActivateWindow(videos,plugin://plugin.video.youtube/?path=/root/search&amp;feed=search&amp;search=,return)</xbox>
      <eject>EjectTray()</eject>
      <star>ActivateWindow(Settings)</star>
      <hash>RunScript(/usr/share/kodi/scripts/kodi_say_time.py)</hash>

    </remote>
  </global>
  <Home>
    <remote>
      <menu>Skin.ToggleSetting(HomeViewToggle)</menu>
      <info>ActivateWindow(SystemInfo)</info>
     <clear>ActivateWindow(Weather)</clear>
      <star>ActivateWindow(Settings)</star>
      <hash>RunScript(/usr/share/kodi/scripts/kodi_say_time.py)</hash>
    </remote>
  </Home>

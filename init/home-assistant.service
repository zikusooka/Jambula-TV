[Unit]
Description=Home Assistant Core
After=multi-user.target alsa-restore.service 


[Service]
Type=simple
User=jambula
EnvironmentFile=/etc/sysconfig/home-assistant-core
ExecStart=/bin/bash -c "/usr/bin/hass-${HOME_ASSISTANT_VERS_STRING} -c /etc/JambulaTV/homeassistant"

[Install]
WantedBy=graphical.target
